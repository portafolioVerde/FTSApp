        android:completionThreshold="0"
        android:ems="12"
        android:fontFamily="@font/chelsea_market"
        android:gravity="center_horizontal"
        android:hint="@string/razon_social_title"
        android:inputType="textAutoComplete"
        android:padding="5dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline50"
        app:layout_constraintEnd_toStartOf="@+id/guideline44"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/guideline38"
        app:layout_constraintTop_toTopOf="@+id/guideline48" />

    <EditText
        android:focusedByDefault="true"
        android:id="@+id/et_prov"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bordes_title_01"
        android:ems="12"
        android:fontFamily="@font/chelsea_market"
        android:gravity="center_horizontal"
        android:hint="@string/proveedorDefault_title"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline49"
        app:layout_constraintEnd_toStartOf="@+id/guideline44"
        app:layout_constraintStart_toStartOf="@+id/guideline38"
        app:layout_constraintTop_toTopOf="@+id/guideline45" />

    <EditText
        android:id="@+id/et_nit"
