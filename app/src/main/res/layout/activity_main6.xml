        android:padding="5dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline48"
        app:layout_constraintEnd_toStartOf="@+id/guideline44"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/guideline38"
        app:layout_constraintTop_toTopOf="@+id/guideline49" />
6
    <Button
        android:id="@+id/btn_fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bordes_title_01"
        android:digits=""
        android:ems="12"
        android:fontFamily="@font/chelsea_market"
        android:gravity="center_horizontal"
        android:hint="@string/presiona_para_ingresar_una_fecha"
        android:padding="5dp"
        android:textColorHint="@color/colorAccent"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline47"
        app:layout_constraintEnd_toEndOf="@+id/et_lugar"
        app:layout_constraintStart_toStartOf="@+id/et_lugar"
        app:layout_constraintTop_toTopOf="@+id/guideline51" />

    <EditText
        android:id="@+id/et_lugar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bordes_title_01"
        android:digits="@string/abc"
        android:ems="12"
        android:fontFamily="@font/chelsea_market"
        android:gravity="center_horizontal"
        android:hint="@string/lugar_title"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline51"
        app:layout_constraintEnd_toStartOf="@+id/guideline44"
        app:layout_constraintStart_toStartOf="@+id/guideline38"
        app:layout_constraintTop_toTopOf="@+id/guideline50" />

    <ImageButton
        android:id="@+id/btnSiguiente6"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/bordes_title_01"
        app:layout_constraintBottom_toTopOf="@+id/guideline37"
        app:layout_constraintEnd_toStartOf="@+id/guideline44"
        app:srcCompat="@drawable/ic_arrow_forward" />

    <ImageButton
        android:id="@+id/btnAtras6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/bordes_title_01"
        app:layout_constraintBottom_toTopOf="@+id/guideline37"
        app:layout_constraintStart_toStartOf="@+id/guideline38"
        app:srcCompat="@drawable/ic_arrow_back" />

    <TextView
